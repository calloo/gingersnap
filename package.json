{
  "name": "gingersnap",
  "version": "0.23.2",
  "description": "A general purpose library built with the aim of filling the gaps of javascript shortcomings",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "publish": "cp README.md lib && cd lib && npm publish",
    "lint": "eslint",
    "test": "NODE_OPTIONS=--unhandled-rejections=warn jest",
    "test:win": "set NODE_OPTIONS=--unhandled-rejections=warn && jest",
    "test-with-coverage": "NODE_OPTIONS=--unhandled-rejections=warn jest --coverage",
    "compile": "tsc",
    "docs:internal:build": "typedoc",
    "docs:external:dev": "vitepress dev docs/external",
    "docs:external:build": "vitepress build docs/external && cp -r docs/external/images public/images",
    "docs:external:preview": "vitepress preview docs/external"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitea.iamdrscript.com/CookieNerds/Gingersnap.git"
  },
  "author": "CookieNerds LLC",
  "bugs": {
    "url": "https://gitea.iamdrscript.com/CookieNerds/Gingersnap/issues"
  },
  "type": "module",
  "homepage": "https://calloo.github.io/gingersnap",
  "devDependencies": {
    "@mxssfd/typedoc-theme": "^1.0.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/object-hash": "^3.0.6",
    "@types/papaparse": "^5.3.5",
    "@types/ramda": "^0.30.2",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "blob-polyfill": "^7.0.20220408",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mock-socket": "^9.3.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "rollup": "^3.26.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.4",
    "vitepress": "^1.0.0-alpha.46",
    "vue": "^3.2.47",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "browser-or-node": "^3.0.0-pre.0",
    "modern-isomorphic-ws": "^1.0.5",
    "object-hash": "^3.0.0",
    "papaparse": "^5.4.1",
    "protobufjs": "^7.2.6",
    "ramda": "^0.30.1",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.6.3",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "x2js": "^3.4.4"
  }
}