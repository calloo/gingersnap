{
  "name": "@cookienerds/gingersnap",
  "version": "1.0.5",
  "description": "framework for handling network requests",
  "main": "index.js",
  "scripts": {
    "build": "npm run remove-old-build && npm run compile && npm run remove-test-build && npm run restructure-build && npm run copy-package-json",
    "copy-package-json": "cp package.json lib",
    "remove-test-build": "rm -rf lib/tests && rm -rf lib/**/__mocks__/*",
    "remove-old-build": "rm -rf lib",
    "restructure-build": "mv lib/src/* lib && rm -rf lib/src",
    "lint": "eslint",
    "test": "jest",
    "test-with-coverage": "jest --coverage",
    "compile": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/cookienerd-frameworks/gingersnap.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/cookienerd-frameworks/gingersnap/issues"
  },
  "homepage": "https://gitlab.com/cookienerd-frameworks/gingersnap#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.3",
    "@types/ramda": "^0.28.15",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "babel-jest": "^29.0.3",
    "blob-polyfill": "^7.0.20220408",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.3",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "node-fetch": "^3.2.10",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "x2js": "^3.4.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "coverageReporters": ["html", "text", "text-summary", "cobertura"],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
