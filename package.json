{
  "name": "@cookienerds/gingersnap",
  "version": "4.0.02",
  "description": "framework for handling network requests",
  "main": "dist/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "npm run remove-old-build && npm run compile && npm run remove-test-build && npm run restructure-build && npm run copy-package-json && npm run cleanup",
    "copy-package-json": "cp package.json lib",
    "remove-test-build": "rm -rf lib/tests && rm -rf lib/**/__mocks__/*",
    "remove-old-build": "rm -rf lib",
    "restructure-build": "mv lib/src/* lib",
    "cleanup": " rm -rf lib/src",
    "pack": "webpack",
    "docs": "typedoc",
    "lint": "eslint",
    "test": "jest",
    "test-with-coverage": "jest --coverage",
    "compile": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/cookienerd-frameworks/gingersnap.git"
  },
  "author": "CookieNerds LLC",
  "bugs": {
    "url": "https://gitlab.com/cookienerd-frameworks/gingersnap/issues"
  },
  "homepage": "https://gitlab.com/cookienerd-frameworks/gingersnap#readme",
  "devDependencies": {
    "@mxssfd/typedoc-theme": "^1.0.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/papaparse": "^5.3.5",
    "@types/ramda": "^0.28.15",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "blob-polyfill": "^7.0.20220408",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-websocket-mock": "^2.4.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "whatwg-fetch": "^3.6.2"
