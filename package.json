    "object-hash": "^3.0.0",
    "papaparse": "^5.3.2",
    "protobufjs": "^7.2.2",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0",
    "ws": "^8.11.0",
    "x2js": "^3.4.3"
  }
